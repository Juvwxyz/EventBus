<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Authors>Shoming</Authors>
		<Copyright>Copyright (c) 2024 XmmShp</Copyright>
		<PackageId>EventBusNet8</PackageId>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryUrl>https://github.com/XmmShp/EventBusNet8.git</RepositoryUrl>
		<PackageTags>Event;EventBus;PluginSystem;</PackageTags>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Version>1.2.0</Version>
		<PackageProjectUrl>https://www.nuget.org/packages/EventBus</PackageProjectUrl>
		<Title>EventBus</Title>
	</PropertyGroup>

	<ItemGroup>
	  <Compile Remove="Fundamental\**" />
	  <EmbeddedResource Remove="Fundamental\**" />
	  <None Remove="Fundamental\**" />
	</ItemGroup>

	<ItemGroup>
		<None Include="..\..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

</Project>
